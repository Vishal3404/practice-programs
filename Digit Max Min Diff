import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int min=10,max=0;
        while(n!=0){
            int d=n%10;
            if(d>max){
                max=d;
            }
            else if(d<min){
                min=d;
            }
            n=n/10;
        }
        System.out.print(max-min);
    }
}
