import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        String s=in.nextLine();
        int max=0;
        String res="";
        String w[]=s.split(" ");
        for(int i=0;i<w.length;i++){
            if(w[i].length()>max){
                max=w[i].length();
                res=w[i];
            }
        }
        System.out.print(res);
    }
}
