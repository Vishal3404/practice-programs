import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        String s=in.next();
        String t=in.next();
        int count=0;
        for(int i=0;i<=s.length()-t.length();i++){
            if(s.substring(i,i+t.length()).equals(t)){
                count++;
            }
        }
        System.out.print(count);
    }
}
