import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int a=in.nextInt();
        int b=in.nextInt();
        int k=in.nextInt();
        int num=1;
        for(int i=0;i<b;i++){
            num=num*a;
        }
        String res=""+num;
        if(k>=0&&k<=res.length()){
            System.out.print(res.charAt(res.length()-k));
        }
    }
}
