import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int pow=in.nextInt();
        int r=1;
        for(int i=0;i<pow;i++){
            r=r*n;
        }
        System.out.print(r);
    }
}
