import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int c=0,res=0;
        for(int i=1;i<=n;i++){
            //c=0;
            if(n%i==0){
                c++;
            }
        }
        if(c==3){
            System.out.print("1");
        }
        else{
            System.out.print("0");
        }
    }
}
