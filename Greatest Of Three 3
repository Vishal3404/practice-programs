import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String args[]) {
        Scanner ob=new Scanner(System.in);
        int a=ob.nextInt();
        int b=ob.nextInt();
        int c=ob.nextInt();
        int lar=(a>=b&&a>=c)?a:(b>=a&&b>=c)?b:c;
        System.out.println(lar);
    }
}
